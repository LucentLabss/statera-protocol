{
  "name": "ada-statera-protocol",
  "version": "0.1.0",
  "license": "MIT",
  "type": "module",
  "main": "index.js",
  "packageManager": "yarn@4.0.0",
  "exports": {
    ".": {
      "default": "./dist/index.js",
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts",
      "require": "./dist/index.js"
    }
  },
  "scripts": {
    "compact": "run-compactc ./src/adaStateraProtocol.compact ./src/managed/adaStateraProtocol",
    "build": "rm -rf dist && tsc --project tsconfig.build.json && cp -R ./src/managed ./dist/managed && cd ./src/adaStateraProtocol.compact dist",
    "lint": "eslint src",
    "typecheck": "tsc -p tsconfig.json --noEmit",
    "prepack": "yarn build"
  },
  "devDependencies": {
    "@statera/compact": "workspace:*",
    "eslint": "^9.27.0",
    "jest": "^29.7.0",
    "typescript": "^5.8.2"
  }
}
